datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Emails {
  id      String @id @default(uuid())
  name    String
  email   String
  message String
}

model User {
  id                 String   @id @default(uuid())
  username           String   @unique
  password           String
  firstName          String?
  lastName           String?
  email              String?  @unique
  revokeTokensBefore DateTime @default(now())
}
